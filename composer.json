{
    "name": "teusbarros/currencyexchange",
    "description": "A currency exchange package for the Laravel framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateus Barros",
            "email": "smateusbp@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^8.2",
        "nunomaduro/phpinsights": "^2.8",
        "zircote/swagger-php": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "teusbarros\\CurrencyExchange\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "teusbarros\\CurrencyExchange\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "teusbarros\\CurrencyExchange\\CurrencyExchangeServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
